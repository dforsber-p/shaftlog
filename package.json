{
  "name": "shaftlog",
  "version": "0.0.3",
  "description": "Secure, highly available, and fault tolerant log transport",
  "keywords": ["log", "logging"],
  "bugs": "http://github.com/poplatek/shaftlog/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/poplatek/shaftlog.git"
  },
  "bin": {
    "shaftlog-client": "./shaftlog-client.js",
    "shaftlog-server": "./shaftlog-server.js"
  },
  "devDependencies": {
    "streamline": ">=0.4.7",
    "browserify": "*",
    "glob": "~3",
    "minimatch": "*",
    "backoff": ">=2.2.0",
    "forever-agent": ">=0.4.0",
    "commander": "~1",
    "js-yaml": "~2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "prepublish": "./node_modules/.bin/_node -c lib && ./node_modules/.bin/browserify -o shaftlog-client.js --bare --dg false ./lib/clie.js && ./node_modules/.bin/browserify -o shaftlog-server.js --bare --dg false ./lib/serv.js"
  },
  "preferGlobal": true
}
